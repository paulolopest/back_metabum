{
    "version": 2,
    "builds": [
        {
            "src": "src/index.ts",
            "use": "@vercel/node" 
        }
    ],
    "routes": [
        {
            "src": "(.*)",
            "dest": ["src/Router/User/UserRouter.ts", "src/Router/Cart/CartRouter.ts", "src/Router/Product/ProductRouter.ts", "src/Router/Card/CardRouter.ts", "src/Router/FavoriteProduct/FavoriteProductRouter.ts"]
        }
    ]
}